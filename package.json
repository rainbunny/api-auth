{
  "author": "Thinh Tran",
  "bugs": {
    "url": "https://github.com/rainbunny/auth-api/issues"
  },
  "dependencies": {
    "@rainbunny/api-core": "^0.2.5",
    "@rainbunny/pg-extensions": "^0.4.0",
    "apollo-server": "^2.23.0",
    "apollo-server-express": "^2.23.0",
    "awilix": "^4.3.3",
    "axios": "^0.21.1",
    "axios-observable": "^1.1.6",
    "bunyan": "^1.8.15",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^8.4.1",
    "firebase-admin": "^9.6.0",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "pg": "^8.5.1",
    "querystring": "^0.2.1",
    "rxjs": "^6.6.7",
    "yup": "^0.32.9"
  },
  "description": "Api of auth service. The package is written in Typescript",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bunyan": "^1.8.6",
    "@types/graphql-fields": "^1.3.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.8",
    "@types/pg": "^7.14.11",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-null": "^1.0.2",
    "jest": "^26.6.3",
    "mockdate": "^3.0.5",
    "nodemon": "^2.0.7",
    "prettier": "2.3.0",
    "typescript": "^4.0.5"
  },
  "homepage": "https://github.com/rainbunny/auth-api#readme",
  "keywords": [
    "postgres",
    "typescript",
    "nodejs",
    "graphql"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@rainbunny/auth-api",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainbunny/auth-api.git"
  },
  "scripts": {
    "build": "rm -r -f dist && babel src --out-dir dist --extensions \".ts\" --source-maps inline --copy-files",
    "build-docker": "docker-compose build",
    "dev": "yarn watch & nodemon -r dotenv/config --watch ./dist --nolazy ./dist/server.js",
    "execute": "yarn build && node -r dotenv/config ./dist/scripts/index.js",
    "lint": "prettier --check . && eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit",
    "lint-fix": "prettier --write . && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "push": "git push && git push --tags",
    "test": "jest",
    "k8s-config-map-dev": "kubectl create configmap auth-api-dev-config --from-env-file .env -o yaml --dry-run=client | kubectl apply -f -",
    "k8s-config-map-prod": "kubectl create configmap auth-api-prod-config --from-env-file .env -o yaml --dry-run=client | kubectl apply -f -",
    "k8s-deploy-dev": "kubectl apply -f k8s.deployment.dev.yaml",
    "k8s-deploy-prod": "kubectl apply -f k8s.deployment.prod.yaml",
    "validate": "yarn lint && yarn test",
    "watch": "babel src --out-dir dist --extensions \".ts\" --source-maps inline --copy-files --watch"
  },
  "types": "dist/index.d.ts",
  "version": "0.0.1"
}
